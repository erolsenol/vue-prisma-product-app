{
  "name": "fastify-product-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "tsc --resolveJsonModule -p tsconfig.json",
    "start": "node build/src/index.js | pino-pretty --colorize",
    "dev": "ts-node-dev ./src/index.ts",
    "prebuild": "npm run prisma:generate",
